doctype html
html(lang='es')
  head
    meta(charset='utf-8')
    title Mars Spacelines

    block metas
      meta(http-equiv='X-UA-Compatible', content='IE=edge')
      meta(name='description', content=description)
      meta(name='viewport', content='width=device-width, initial-scale=1')

    block css
      link(rel='stylesheet', href='/css/styles.css')
      link(rel='stylesheet', href='https://fonts.googleapis.com/css?family=Roboto:400,700')

    block js-head
  body

    // ******************************** Navegador ****************
    block navegador
      nav.navbar.navbar-inverse.navbar-fixed-top(role="navigation")
        .container
          // *** Menú comprimido.  Sólo muestra la marca y un botón que revelará el menú inferior
          .navbar-header.page-scroll
            button(type="button", class="navbar-toggle", data-toggle="collapse", data-target="#menu-links")
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            // Título de la web o nombre/imagen de la marca
            a.navbar-brand(href="#") Mars Spacelines

          // *** Menú desplegado
          .collapse.navbar-collapse#menu-links
            ul.nav.navbar-nav.navbar-right
              // La sección activa se aplica al li
              li.hidden
                a(href="#")
              li.active
                a(href="#") Inicio
              li
                a(href="#") Marte
              li
                a(href="#") El viaje
              li
                a(href="#") Contacto

    // ******************************** Contenido principal ****************
    block contenido
      header.jumbotron.img-header
        .text-head.vcenter
          h1 Mars Spacelines
